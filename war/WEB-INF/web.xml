<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  	<!-- Servlets mysql -->
  	<servlet>
    	<servlet-name>serviceServlet</servlet-name>
    	<servlet-class>com.project.titulo.server.ServerServiceImpl</servlet-class>
  	</servlet>
  
  	<servlet-mapping>
    	<servlet-name>serviceServlet</servlet-name>
    	<url-pattern>/proyectotitulo/service</url-pattern>
  	</servlet-mapping>
  
   
  <servlet>
    <servlet-name>jUnitHostImpl</servlet-name>
    <servlet-class>com.google.gwt.junit.server.JUnitHostImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>jUnitHostImpl</servlet-name>
    <url-pattern>/testgwtjavaplot/remote_logging</url-pattern>
  </servlet-mapping>
  <!-- Mail Servlet -->

  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Proyectotitulo.html</welcome-file>
  </welcome-file-list>
  
</web-app>
