<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:gwt="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.resumePanel{ 
	    color: #d6092c;
	    border: none;
	    font-size: 12px;
	    text-transform: uppercase;
	    font-weight: 700;
	    font-family: Roboto,'Helvetica Neue',Helvetica,Arial,sans-serif;   
    	padding:25px;
	}
	
	</ui:style>
	
	<gwt:HTMLPanel>

		<gwt:VerticalPanel addStyleNames='{style.resumePanel}' width="100%" verticalAlignment="ALIGN_MIDDLE">
			<gwt:InlineLabel text="Administration Panel"/>
		</gwt:VerticalPanel>
	</gwt:HTMLPanel>
	
</ui:UiBinder> 