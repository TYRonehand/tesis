<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.panel404{
		padding:28px;
	}
	.title404{
		color: #333;
		font-family: Roboto,'Helvetica Neue',Helvetica,Arial,sans-serif;  
		font-size: 45px;
		margin: 0 0 10px;
    	font-weight: 700;
    	line-height: 1.1;
    	color: inherit;
	}
	.text404{
		color: #333;
		font-family: Lato,sans-serif;;  
		font-size: 24px;
		font-weight: 600;
		line-height: 36px;
		margin: 0 0 24px;
		text-align: center;
	}
	</ui:style>
	<g:HTMLPanel>
	<div class="row">
		<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
			<section class="panel">
				<div class="panel-body">

					<div class="btn-group btn-group-justified">
						
						<g:VerticalPanel ui:field="panel404" width="100%" addStyleNames='{style.panel404}' horizontalAlignment="ALIGN_CENTER" height="100%">
							<g:Image url="assets/img/logofix2.svg" width= "65%"/>
							<g:Image url="assets/img/warning.png" width= "290px"/>
							<g:Label addStyleNames='{style.title404}' horizontalAlignment="ALIGN_CENTER" width="100%" text="Error 404"></g:Label>
							<g:Label addStyleNames='{style.text404}' horizontalAlignment="ALIGN_CENTER" width="100%" text="This page is not available on the site. You probably entered a broken link or the page you are looking for is outdated (error 404)."></g:Label>
						</g:VerticalPanel>
						
			
					</div>
				</div>
			</section>
		</div>
	</div>
	</g:HTMLPanel>
</ui:UiBinder> 