<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:gwt="urn:import:com.google.gwt.user.client.ui" xmlns:w="urn:import:com.google.gwt.widget.client">
	<ui:style>
	.HTMLheader {
		padding-top: 120px;
	}
	</ui:style>
	<gwt:HTMLPanel addStyleNames='{style.HTMLheader}'>

		<gwt:VerticalPanel ui:field="backbreadPanel"  width="100%" verticalAlignment="ALIGN_MIDDLE">
			<gwt:HorizontalPanel verticalAlignment="ALIGN_MIDDLE" spacing="4">
				<gwt:Image url="assets/img/icons/home_white.png" width="25px" height="25px"/>
				<gwt:FlowPanel ui:field="breadPanel"/>
			</gwt:HorizontalPanel>
			
		</gwt:VerticalPanel>



	</gwt:HTMLPanel>
</ui:UiBinder> 